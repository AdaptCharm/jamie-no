{
  "name": "jamie-no",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "analyze": "BUNDLE_ANALYZE=both yarn build",
    "prettier-fix": "prettier --write .",
    "find:unused": "next-unused"
  },
  "dependencies": {
    "@headlessui/react": "^1.4.1",
    "@heroicons/react": "^1.0.4",
    "autoprefixer": "^10.3.3",
    "clsx": "^1.1.1",
    "mousetrap": "^1.6.5",
    "next": "^11.1.2",
    "next-seo": "^4.26.0",
    "next-themes": "^0.0.15",
    "postcss": "^8.3.6",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-popper": "^2.2.5"
  },
  "prettier": {
    "singleQuote": true,
    "semi": false
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^11.1.2",
    "@types/mousetrap": "^1.6.8",
    "@types/react": "^17.0.19",
    "@types/react-dom": "^17.0.9",
    "husky": "^7.0.2",
    "next-unused": "^0.0.6",
    "prettier": "^2.3.2",
    "tailwindcss": "^2.2.9",
    "typescript": "^4.4.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*.{ts,tsx}": [
      "prettier --write",
      "git add"
    ]
  },
  "next-unused": {
    "alias": {
      "@assets/*": [
        "assets/*"
      ],
      "@config/*": [
        "config/*"
      ],
      "@components/*": [
        "components/*"
      ],
      "@lib/*": [
        "lib/*"
      ]
    },
    "debug": true,
    "include": [
      "components",
      "pages"
    ],
    "exclude": [],
    "entrypoints": [
      "pages"
    ]
  }
}
